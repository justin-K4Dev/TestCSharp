<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <startup>        
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1"/>
  </startup>

  <runtime>
    <!-- Workstation GC 사용 (기본값) -->
    <gcServer enabled="false" />
    <!-- Background GC 활성화 (권장) -->
    <gcConcurrent enabled="true" />
  </runtime>  

</configuration>

